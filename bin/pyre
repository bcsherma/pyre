#!/usr/bin/env python3
# -*- coding: utf-8 -*-

import click
import pyre


@click.command()
@click.argument("team")
@click.argument("league")
@click.argument("year")
def main(team, league, year):
    """ """
    reader = pyre.EventFileReader(team, year, league)
    reader.data_frame()  # Currently parses and does nothing with result


if __name__ == "__main__":
    main()
